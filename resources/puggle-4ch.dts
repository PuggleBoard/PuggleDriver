/dts-v1/;
/plugin/;

/ {
	compatible = "ti,beaglebone", "ti,beaglebone-black";

	/* identification */
	part-number = "puggle-4ch";

	fragment@0 {
		target = <&am33xx_pinmux>;
		__overlay__ {
			pinctrl_test: pinctrl_test_7_pins {
				pinctrl-single,pins = <
					/*0x084 0x05*/ /* P8_20 OUTPUT | MODE 5*/
					0x0E0 0x05 /* P8_27 OUTPUT | MODE 5*/
					0x0E4 0x05 /* P8_29 OUTPUT | MODE 5*/
					0x0E8 0x05 /* P8_28 OUTPUT | MODE 5*/
					0x0EC 0x05 /* P8_30 OUTPUT | MODE 5*/
					/*0x080 0x06*/ /* P8_21 OUTPUT | MODE 6*/
				>;
			};
		};
	};

	fragment@1 {
		target = <&ocp>;
		__overlay__ {
			test_helper: helper {
				compatible = "bone-pinmux-helper";
				pinctrl-names = "default";
				pinctrl-0 = <&pinctrl_test>;
				status = "okay";
			};
		};
	};
};
